#: reroils_data/jsonschemas/records/record-v0.0.1.json:190
msgid "Additional materials"
msgstr "Materiale di accompagnamento"

#: reroils_data/jsonschemas/records/record-v0.0.1.json:191
msgid "Accompanying material of the resource, ie maps."
msgstr "Materiali che accompagnano la risorsa, ad esempio mappe."

#: reroils_data/jsonschemas/records/record-v0.0.1.json:196
msgid "Series"
msgstr "Collezione"

#: reroils_data/jsonschemas/records/record-v0.0.1.json:197
msgid "Series to which belongs the resource."
msgstr "Collezione cui appartiene la risorsa."

#: reroils_data/jsonschemas/records/record-v0.0.1.json:207
msgid "Title of the series."
msgstr "Titolo della collezione."

#: reroils_data/jsonschemas/records/record-v0.0.1.json:211
msgid "Numbering"
msgstr "Numerazione"

#: reroils_data/jsonschemas/records/record-v0.0.1.json:212
msgid "Numbering of the resource within the series."
msgstr "Numerazione delle risorse all'interno della raccolta."

#: reroils_data/jsonschemas/records/record-v0.0.1.json:219
msgid "Note"
msgstr "Nota"

#: reroils_data/jsonschemas/records/record-v0.0.1.json:220
msgid "Note on the resource."
msgstr "Nota sulla risorsa."

#: reroils_data/jsonschemas/records/record-v0.0.1.json:229
msgid "Abstract"
msgstr "Riassunto"

#: reroils_data/jsonschemas/records/record-v0.0.1.json:230
msgid "Abstract of the resource."
msgstr "Riassunto della risorsa."

#: reroils_data/jsonschemas/records/record-v0.0.1.json:240
msgid "Persistent identifiers of the resource, ie reroID and ISBN."
msgstr "Identificatori perenni della risorsa, ad esempio reroID e ISBN."

#: reroils_data/jsonschemas/records/record-v0.0.1.json:246
msgid "reroID"
msgstr ""

#: reroils_data/jsonschemas/records/record-v0.0.1.json:247
msgid "Corresponding reroID of the original RERO record."
msgstr ""
"reroID corrispondente risvolto del record bibliografico RERO originale."

#: reroils_data/jsonschemas/records/record-v0.0.1.json:252
msgid "bnfID"
msgstr ""

#: reroils_data/jsonschemas/records/record-v0.0.1.json:253
msgid "Corresponding bnfID of the original BNF record."
msgstr "bnfID corrispondente del record bibliografico BNF originale."

#: reroils_data/jsonschemas/records/record-v0.0.1.json:259
msgid "ISBN of the resource."
msgstr "ISBN della risorsa."

#: reroils_data/jsonschemas/records/record-v0.0.1.json:262
msgid "Should be a valid ISBN-13 without dashes."
msgstr "Deve essere un ISBN-13 valido senza trattini."

#: reroils_data/jsonschemas/records/record-v0.0.1.json:267
msgid "Subject"
msgstr "Oggetto"

