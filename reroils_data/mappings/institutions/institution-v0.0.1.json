{
  "settings": {
      "number_of_shards": 1,
      "number_of_replicas": 0,
      "max_result_window": 20000
  },
  "mappings": {
    "institution-v0.0.1": {
      "date_detection": false,
      "numeric_detection": false,
      "properties": {
      "$schema": {
        "type": "string",
          "index": "not_analyzed"
        },
        "institutionid": {
          "type": "integer",
          "index": "not_analyzed"
        },
        "name": {
          "type": "string"        },
        "address": {
          "type": "string"
        },
        "_created" : {
          "type" : "date"
        },
        "_updated" : {
          "type" : "date"
        }
      }
    }
  }
}
